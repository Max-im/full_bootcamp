<% include ./partials/header %>

<main class="main__container container ui main text segment">
    <h1>single page</h1>

    <div class="item">
        <div class="image">
            <img src="<%= data.img %>">
        </div>
        <div class="content">
            <h4 class="header"><%= data.title %></h4>
        <div class="meta">
            <span><%= data.created %></span>
        </div>
        <div class="description">
            <p><%= data.body %></p>
        </div>
    </div>

    
  </div>
</main>


<% include ./partials/footer %>